<%# <h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p> %>

<%= form_for(@song) do |f| %>
    <label>Song Name:</label><br>
    <%= f.text_field :name%>

    <label>Artist ID:</label><br>
    <%= f.number_field :artist_id%>

    <label>Genre ID:</label><br>
    <%= f.number_field :genre_id%>

    <%= f.submit "Update Song"%>
<% end %>