<h1>Artist Form</h1>
<%= form_for @artist do |f| %>
<label>Artists Name: </label>
<%= f.text_field :name %>

<label>Bio: </label>
<%= f.text_field :bio %>

<%= collection_select(:song, :song_id, Song.all, :id, :name) %>

<%= f.submit %>
<% end %>
